#include <stdio.h>

void swapinarr(int a[],int i , int j){
    int t;
    t = a[i];
    a[i] = a[j];
    a[j] = t;
}

void bubblesort(int a[],int size){
    for(int i = 1 ; i < size ;i++){
        for(int j = 0; j < size - i; j++){
            if(a[j+1] > a[j]){
            int t;
            t = a[j];
            a[j] = a[j+1];
            a[j+1] = t;
            }
        }
    }
}

int countduplicates(int a[],int size){
    int cnt = 0 ;
    bubblesort(a,size);
    for(int i = 0 ; i < size-1 ; i++){
        if(a[i] == a[i+1] ) cnt ++;
    }
    return cnt;
}

void printunique(int a[],int size){
    bubblesort(a,size);
    for(int i = 0 ; i < size -1 ; i++){
        if(a[i] != a[i+1]) printf("%d",a[i]);
    }
    if(size == 1 || a[size-2]!=a[size-1]) printf("%d",a[size-1]);
}

void merge_desc(int a[], int b[], int c[], int n)
{
    int i = 0, j = 0, k = 0;

    while(i < n && j < n)
    {
        if(a[i] > b[j])
            c[k++] = a[i++];
        else
            c[k++] = b[j++];
    }

    while(i < n)
        c[k++] = a[i++];

    while(j < n)
        c[k++] = b[j++];
}

void merge_desc(int a[], int b[], int c[], int n)
{
    int i = 0, j = 0, k = 0;

    while(i < n && j < n)
    {
        if(a[i] > b[j])
            c[k++] = a[i++];
        else
            c[k++] = b[j++];
    }

    while(i < n)
        c[k++] = a[i++];

    while(j < n)
        c[k++] = b[j++];
}

void countfrequency(int a[], int size)
{
    bubblesort(a, size);   // reuse your bubble sort

    int count = 1;

    for(int i = 0; i < size - 1; i++)
    {
        if(a[i] == a[i+1])
            count++;
        else
        {
            printf("%d -> %d\n", a[i], count);
            count = 1;
        }
    }

    // print frequency of last element
    printf("%d -> %d\n", a[size-1], count);
}


int main (){
    int a[]= {5,7,9,5,8};
    int b = countduplicates(a,5);
    printf("%d",b);
    return 0;
}
