#include<stdio.h>
#include<string.h>

int cntvowel(char str[]){
    char s[] = "aeiouAEIOU";
    int cnt = 0;
    for(int i = 0 ; str[i] ; i++){
        for(int j = 0 ; j < s[j] ; j++){
            if(str[i] == s[j]){
                cnt++;
                break;
            }
        }
    }
    return cnt ;
}

int findindex(char str[],char s){
    int i ;
    for(i = 0 ; str[i] ;i++){
        if ( s == str[i]){
            return i;
        }
    }
    return -1;
}

int findbetween(char str[],int i, int j, char s){
    for (int k = i + 1; k < j; k++) {
        if (str[k] == s) {
            return k;
        }
    }
    return -1;
}

void f4(char str[],int i ,int j){
    char ch = str [i];
    str[i] = str[j];
    str[j] = ch;
}

int isalpha(char str[]){
    int cnt = 0;
    for(int i = 0 ; str[i] ; i++){
        if((str[i] >= 'a' && str[i] <= 'z') || (str[i] >= 'A' && str[i] <='Z')) cnt++;
        else if (str[i] >= '0' && str[i] <= '9') cnt++ ;
    }
    if(cnt >= 2) return 1;
    return 0;
}

int main (){
    char str[50];
    printf("Enter the string : ");
    fgets(str,50,stdin);
    str[strlen(str)-1] = 0;
    int a = f2(str,'m');
    printf("%d",a);
}
