
void printrevereseevenN(int n){
    if(n>0){
    printf("%d\n",n*2);
    printrevereseevenN(n-1);
    }
}

void printsquareN(int n){
    if(n > 0){
        printsquareN(n-1);
        printf("%d\n",n*n);
    }
}

void DtoB(int n){
    if(n>0){
        DtoB(n/2);
        printf("%d",n%2);
    }
}

void DtoO(int n){
    if(n>0){
        DtoO(n/8);
        printf("%d",n%8);
    }
}

void reverseN(int n){
    if(n>0){
        printf("%d",n%10);
        reverseN(n/10);
    }
}
