/*
int main()
{
    int num,x,digits,digit,i,p,sum;
    printf("Enter a number");
    scanf("%d",&num);
    x=num;

    //count digits (say digits)
    for(digits=0;x;x=x/10)
        digits++;
    // calculate sum of pow(each digit, digits)
    
    for(x=num,sum=0;x;x=x/10)
    {
        digit=x%10;
        for(i=1,p=1;i<=digits;i++)
            p=p*digit;
        sum=sum+p;
    }
    // compare result with num
    if(sum==num)
        printf("Armstrong Number");
    else
        printf("Not an Armstrong Number");
    printf("\n");
    return 0;
}
